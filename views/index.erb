


<ul class="tagsFeed"></ul>

 <div id="itemModal" class="reveal-modal">

  <form method="post" action="/new_potluck">
    Potluck Name:
    <input type="text" name="potluck[name]"><br>
    Potluck Location:
    <input type="text" name="potluck[location]"><br>
    Potluck Date:
    <input type="date" name="potluck[date]"><br><br>

    <button class="button success small">Save Potluck</button>
  </form>
</div>


<script type="text/javascript">
$(document).ready(function() {

  $('.tagsFeed').spectragram('getRecentTagged', {
    query: 'picnic', //this gets the recent photo feed tagged with the word: potluck
    size: 'small',
    min: 16,


  });

});

</script>









